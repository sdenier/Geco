<html>
  <head>
    <style type="text/css">
      body { font-size: 10 }
      h1 { font-size: 20; text-align: center }
      h2 { font-size: 15 }
      table { border-collapse:collapse; width: 100%; font-size: 10 }
      .runner td, .runner th { padding: 0px 10px }
      .left { text-align: left }
      .center { text-align: center }
      .right, .time, .pace, .diff, .controls, .times, .splits { text-align: right }
      .diff, .splits { font-style: italic }
      .controls { background-color: #F0F0F0 }
      .times, .splits { background-color: white }
      .rsplit td, .col0 td, .col1 td { padding: 0 5px 0 0 }
      .rsplit td, .time, .best { font-weight: bold }
    </style>
  </head>
  <body>
{{#geco_ResultsCollection}}
    <h2>{{geco_ResultName}} ({{geco_NbFinishedRunners}}/{{geco_NbPresentRunners}}){{#geco_SingleCourse?}} - {{geco_CourseLength}}m, {{geco_CourseClimb}}m{{/geco_SingleCourse?}}</h2>
    <table>
      <tr class="runner"><th></th><th class="left">{{i18n_NameHeader}}</th><th class="left">{{i18n_ClubHeader}}</th>{{#geco_RunnerCategory?}}<th class="left">{{i18n_CategoryHeader}}</th>{{/geco_RunnerCategory?}}<th class="right">{{i18n_TimeHeader}}</th><th class="right">{{i18n_DiffHeader}}</th>{{#geco_SingleCourse?}}<th class="right">{{i18n_PaceHeader}}</th>{{/geco_SingleCourse?}}{{#geco_Penalties?}}<th class="right">{{i18n_PenaltyHeader}}</th><th class="right">{{i18n_RacetimeHeader}}</th>{{/geco_Penalties?}}</tr>
{{#geco_RankedRunners}}
      <tr class="runner"><td>{{geco_RunnerRank}}</td><td>{{geco_RunnerFirstName}} {{geco_RunnerLastName}}</td><td>{{geco_RunnerClubName}}</td>{{#geco_RunnerCategory?}}<td>{{geco_RunnerCategory}}</td>{{/geco_RunnerCategory?}}<td class="time">{{geco_RunnerResultTime}}</td><td class="diff">{{geco_RunnerDiffTime}}</td>{{#geco_SingleCourse?}}<td class="pace">{{geco_RunnerPace}}</td>{{/geco_SingleCourse?}}{{#geco_Penalties?}}<td class="right">{{geco_RunnerTimePenalty}}</td><td class="right">{{geco_RunnerRaceTime}}</td>{{/geco_Penalties?}}</tr>
{{/geco_RankedRunners}}
      <tr class="empty"><td>&nbsp;</td></tr>
{{#geco_UnrankedRunners}}
      <tr class="runner"><td>{{geco_RunnerRank}}</td><td>{{geco_RunnerFirstName}} {{geco_RunnerLastName}}</td><td>{{geco_RunnerClubName}}</td>{{#geco_RunnerCategory?}}<td>{{geco_RunnerCategory}}</td>{{/geco_RunnerCategory?}}<td class="time">{{geco_RunnerStatusOrTime}}</td><td></td>{{#geco_SingleCourse?}}<td></td>{{/geco_SingleCourse?}}{{#geco_Penalties?}}<td class="right">{{geco_RunnerTimePenalty}}</td><td class="right">{{geco_RunnerRaceTime}}</td>{{/geco_Penalties?}}</tr>
{{/geco_UnrankedRunners}}
    </table>
{{/geco_ResultsCollection}}
  </body>
</html>
